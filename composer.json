{
    "name": "jason/wang-editor",
    "description": "This is my package wang-editor",
    "keywords": [
        "Jason.Chen",
        "laravel",
        "wang-editor"
    ],
    "homepage": "https://github.com/jason/wang-editor",
    "support": {
        "issues": "https://github.com/jason/wang-editor/issues",
        "source": "https://github.com/jason/wang-editor"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jason",
            "email": "chenjxlg@163.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/forms": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "autoload": {
        "psr-4": {
            "Jason\\WangEditor\\": "src/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jason\\WangEditor\\WangEditorServiceProvider"
            ],
            "aliases": {
                "WangEditor": "Jason\\WangEditor\\Facades\\WangEditor"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
